<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
		<rewrite>
		  <rules>
			<rule name="ReactRouter Routes" stopProcessing="true">
			  <match url=".*" />
			  
			  <action type="Rewrite" url="http://localhost:3000/{R:1}" />
			</rule>

		  </rules>
		</rewrite>
    </system.webServer>
</configuration>